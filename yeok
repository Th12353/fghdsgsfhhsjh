--

game:GetService("RunService").RenderStepped:Connect(function()
    if game.Workspace.Monster.A.Torso.Transparency == 0 then
        Torso.Visible = true
        AHead.Visible = true
        RArm.Visible = true
        RLeg.Visible = true
        LLeg.Visible = true
        LArm.Visible = true
        
        BillboardGui.Enabled = true
    else
        Torso.Visible = false
        AHead.Visible = false
        RArm.Visible = false
        RLeg.Visible = false
        LLeg.Visible = false
        LArm.Visible = false
        
        BillboardGui.Enabled = false
    end
end)
